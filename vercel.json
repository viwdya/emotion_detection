{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}
{
  "build": {
    "commands": [
      "pip install --upgrade pip setuptools wheel",
      "pip install --target . --upgrade -r requirements.txt"
    ]
  }
}

